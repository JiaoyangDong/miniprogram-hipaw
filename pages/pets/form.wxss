.main {
  background: #ffffff;
  display: flex;
  justify-content: center;
  margin-top: 20rpx;
  height: 100vh;
  font-family: Montserrat;
}

.form-box {
  color: #404040;
  font-size: 12pt;
}

.form-item {
  margin-bottom: 10rpx;
}

input {
  background-color: #F0EFEF;
  border-radius: 20rpx;
  padding: 14rpx;
  margin-top: 14rpx;
  padding-left: 40rpx;
  font-family: Montserrat;
  font-size: 12pt;
}
textarea {
  background-color: #F0EFEF;
  border-radius: 20rpx;
  margin-top: 14rpx;
}
